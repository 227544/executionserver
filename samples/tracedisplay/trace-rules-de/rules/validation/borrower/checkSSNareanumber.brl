<?xml version="1.0" encoding="UTF-8"?>
<model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>checkSSNareanumber</name>
  <uuid>9340ec91-7336-4c11-84da-57debed25927</uuid>
  <status>new</status>
  <locale>en_US</locale>
  <definition><![CDATA[if 
    the area number of the SSN of 'the borrower' is one of { "000" , "666" } 
then 
    in 'the loan report' , reject the data with the message "The borrower's SSN area number cannot be " + the area number of the SSN of 'the borrower'; 

]]></definition>
</model.brl:ActionRule>
